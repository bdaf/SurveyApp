<ul id="surveys_list">
  <% surveys.each do |survey| %>
    <li>
      <%= link_to survey.id, survey %>
      Age: 
      <%= survey.age %>
      Growth: 
      <%= survey.growth %>
      Gender: 
      <%= survey.gender %>
      Favourite Color: 
      <%= survey.fav_color %>
    </li>
    
  <% end %>
</ul>